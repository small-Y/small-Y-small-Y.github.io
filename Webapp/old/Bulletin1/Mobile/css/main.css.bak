body, html {
  height: 100%;
  -webkit-tap-highlight-color: transparent;
}


.weui_title_bar {
	position: absolute;
    top: 0;
    left: 0;
    right: 0;
    font-size: 12pt;
    text-align: center;
    padding: 0px;
    height: 43px;
    line-height: 44px;
	color:#333;
	background: -webkit-linear-gradient(#ffffff, #fdfdfd);
    background: -o-linear-gradient(#ffffff, #fdfdfd);
    background: -moz-linear-gradient(#ffffff, #fdfdfd);
    background: linear-gradient(#ffffff, #fdfdfd);
    border-bottom: 1px solid #dcdbdb;
    z-index: 2;
}

.weui_title_bar .back{
	color:#666;
	float:left;
    line-height: 44px;
	padding: 0 10px;
}

.weui_title_bar .btn{
	color: #009688;
    float: right;
    line-height: 40px;
    padding: 0 10px 2px 10px;
}

.weui_title_bar .btn i{
	font-size: 16.5pt;
}

.weui_tab_body {
	position: absolute;
    top: 43px;
    left: 0;
    right: 0;
    bottom: 0px;
    overflow: hidden;
}

.weui_tab_body.blur{
	-webkit-filter: blur(3px);
	-moz-filter: blur(3px);
	-o-filter: blur(3px);
	-ms-filter: blur(3px);
	filter: blur(3px);
}

.weui_tab_body .content{
	position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    overflow: auto;
    -webkit-overflow-scrolling: touch;
}

.weui_tab_main{
	position: absolute;
    top: 0px;
    left: 100%;
    width: 100%;
    bottom: 0px;
	background: #f7f7fa;
	z-index: 3;
    overflow: hidden;
    display: none;
}

.weui_tab_main .subtitle_bar{
	position: absolute;
    top: 0px;
    left: 0px;
    right: 0px;
    height: 43px;
    padding: 0px;
	background: -webkit-linear-gradient(#ffffff, #fdfdfd);
    background: -o-linear-gradient(#ffffff, #fdfdfd);
    background: -moz-linear-gradient(#ffffff, #fdfdfd);
    background: linear-gradient(#ffffff, #fdfdfd);
    border-bottom: 1px solid #dcdbdb;
    text-align: center;
	font-size: 12pt;
}

.weui_tab_main .subtitle_bar .back{
	color:#666;
	float:left;
    line-height: 44px;
	padding: 0 10px;
}

.weui_tab_main .subtitle_bar .title{
	color:#666;
	font-size: 12pt;
    line-height: 44px;
}


.weui_tab_main .subtitle_bar .ok{
	color:#009688;
	float:right;
    line-height: 44px;
    padding: 0 10px;
}

.weui_tab_main .subtitle_bar .ok i{
    font-size: 18pt;
}

.weui_tab_main .subtitle_bar .badge:empty {
    display: none;
}

.weui_tab_main .subtitle_bar .badge{
	position: relative;
    top: -10px;
    left: -5px;
    color: #fff;
    font-size: 9px;
    padding: 0px 4px;
    text-align: center;
    background: rgba(244,67,54,0.7);
	border-radius: 12px;
	-moz-border-radius: 12px;
	-webkit-border-radius: 12px;
}

.weui_tab_main .subtitle_body{
	position: absolute;
    top: 44px;
    left: 0;
    right: 0;
    bottom: 0px;
	z-index: 3;
    -webkit-overflow-scrolling: touch;
}

.weui_tab_main .subtitle_body .content{
	position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0px;
    background: #fcfcfc;
    overflow: auto;
    -webkit-overflow-scrolling: touch;
}

.weui_tab_main .subtitle_body .weui_article .title{
	font-size: 16pt;
    font-weight: bold;
    color: #000;
}

.weui_tab_main .subtitle_body .weui_article .time{
    color: #ccc;
}

.weui_panel_access .from{
	color:#333;
    font-size: 13pt;
}

.weui_panel_access .badge-danger:empty {
    display: none;
}

.weui_panel_access .badge-danger{
	color: #fff;
	font-size: 12px;
    padding: 0 6px;
    text-align: center;
    background: rgba(244,67,54,0.7);
	border-radius: 12px;
	-moz-border-radius: 12px;
	-webkit-border-radius: 12px;
}

.weui_panel_access .close{
	float: right;
	color: #fff;
	font-size: 12px;
    padding: 2px 10px;
	background: rgba(5,169,114,0.7);
	border-radius: 12px;
	-moz-border-radius: 12px;
	-webkit-border-radius: 12px;
	cursor:pointer;
}

.weui_panel_access .time{
	float: right;
}

.weui_search_bar{
	padding: 0 5px 0px 0px;
	background: -webkit-linear-gradient(#fcfcfc,#efeff4); /* Safari 5.1 - 6.0 */
	background: -o-linear-gradient(#fcfcfc,#efeff4); /* Opera 11.1 - 12.0 */
	background: -moz-linear-gradient(#fcfcfc,#efeff4); /* Firefox 3.6 - 15 */
	background: linear-gradient(#fcfcfc,#efeff4); /* 标准的语法 */
}

.weui_search_bar.mb{
	background: -webkit-linear-gradient(#ffffff, #fdfdfd);
    background: -o-linear-gradient(#ffffff, #fdfdfd);
    background: -moz-linear-gradient(#ffffff, #fdfdfd);
    background: linear-gradient(#ffffff, #fdfdfd);
}

.weui_search_bar .title{
	width: 100%;
    height: 43px;
    line-height: 44px;
    text-align: center;
	color: #666;
}

.weui_search_bar .btn{
	width: 40px;
    height: 44px;
    line-height: 42px;
    text-align: center;
	color: #009688;
	cursor: pointer;
}

.weui_search_bar .btn i{
    font-size: 16.5pt;
}

.weui_search_outer{
	margin: 8px 0 8px 8px;
}


.weui_search_cancel {
    line-height: 44px;
}

.weui_tabbar {
   height: 50px;
}

.weui_tabbar_icon i{
    font-size: 18pt;
}

.weui_tabbar_icon {
	color: #737383;
	height: 20px;
	margin-top: 2px;
}

.weui_tabbar_label {
	padding: 3px 0 0 0;
	color: #737383;
}


.weui_tabbar_item{
	padding: 0;
}

.weui_tabbar_item.weui_bar_item_on .weui_tabbar_icon {
	color: #05a972;
}

.weui_tabbar_item.weui_bar_item_on .weui_tabbar_label {
	color: #05a972;
}

