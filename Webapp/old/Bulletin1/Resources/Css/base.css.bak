html {
	overflow: hidden;
	height:100%;
}
body {
	width: 100%;
	height: 100%;
	font-family: Arial,Tahoma,微软雅黑;
	font-size: 12px;
	overflow: hidden;
}

#startingCover {
	position: absolute;
	left: 0;
	top: 0;
	z-index: 1000000;
	width: 100%;
	height: 100%;
	overflow: hidden;
	background: url(../images/tiles/random_grey_variations.png);
}

#startingLogo {
	position: absolute;
	left: 50%;
	top: 50%;
	margin: -80px 0 0 -62px;
	width: 124px;
	height: 124px;
	background: url(../images/waiting.gif);
}

#startingText {
	position: absolute;
	left: 50%;
	top: 50%;
	margin: 55px 0 0 -80px;
	width: 160px;
	height: 50px;
	font-family: Arial,Tahoma,微软雅黑;
	font-size: 13.5px;
	color: white;
	text-shadow: #666 1px 1px 2px;
	text-align: center;
	cursor: default;
}

#startingVer{
	position: absolute;
	left: 50%;
	top: 100%;
	margin: -25px 0 0 -100px;
	width: 200px;
	height: 25px;
	font-family: Arial,Tahoma,微软雅黑;
	font-size: 10.5px;
	color: #ffffff;
	opacity: 0.75;
	text-shadow: #666 1px 1px 2px;
	text-align: center;
	cursor: default;
}

#mainFrame{
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	overflow: hidden;
	background: #fafafa;
	padding: 12px;
	display: none;
}

#bulletinList li{
	list-style-type:none;
}

#bulletinList li a{
  display: block;
  padding: 5px 10px !important;
  clear: both;
  font-weight: normal;
  line-height: 20px;
  white-space: normal !important;
}

#bulletinList li i{
  margin-right: 3px;
}

#bulletinList li a{
   font-size: 13px;
   padding: 10px !important;
   background-color: #ffffff;
}

#bulletinList li a.selected,
#bulletinList li a:hover {
  background-image: none;
  background-color: #f5f5f5;
  color: #000;
  filter:none;
  text-decoration:none
}

#bulletinList li p{
  padding: 10px;
  background-color: #eee;
  margin: 0px;
  font-size: 14px;
  font-weight: 300;
  color: #000;
}

#bulletinList li a{
  padding: 7px 0 5px 0px;
  list-style: none;
  border-bottom: 1px solid #eee !important;
  font-size: 12px;
  text-shadow: none;
}

#bulletinList li:first-child a {
  border-top: none;
  border-bottom: 1px solid #eee !important;
}

#bulletinList li:last-child a {
  border-top: 1px solid white !important;
  border-bottom: 1px solid #eee !important;
}

#bulletinList li > a .photo {
  float: left;
  padding-right: 6px;
}

#bulletinList li > a .photo > img {
  height: 40px;
  width: 40px;
}

#bulletinList li > a .photo-sm {
  float: left;
  padding-right: 6px;
}

#bulletinList li > a .photo-sm > img {
  height: 20px;
  width: 20px;
}

#bulletinList li > a .subject {
  display: block;
}

#bulletinList li > a .subject .from {
  font-size: 14px;
  font-weight: 400;
  color: #02689b;
}

#bulletinList li > a .subject .time {
  font-size: 12px;
  font-weight: 600;
  font-style: italic;
  position: relative;
  float: right;
}

#bulletinList li > a .message {
  display: block !important;
  font-size: 12px;
  color:#000;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

#bulletinInfo .actions{
	position: absolute;
	top: 10px;
	width: 60px;
	right: 10px;
	height: 30px;
	text-align: right;
}

#bulletinInfo .title{
	position: absolute;
	top: 0px;
	left: 0;
	right: 0;
	height: 60px;
	text-align: center;
    font-size: 24pt;
    color: #607D8B;
    padding: 0px 10px 0 20px;
}

#bulletinInfo .title hr{
	margin: 0;
    border: 2px solid #9E9E9E;
}

#bulletinInfo .content{
	position: absolute;
	top: 60px;
	left: 0;
	right: 0;
	bottom: 140px;
    padding: 0px 10px 10px 10px;
	text-align: left;
	overflow: auto;
}

