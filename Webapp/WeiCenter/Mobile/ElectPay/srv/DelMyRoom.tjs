<!--#INCLUDE FILE="/AppEngine/CoreExt/ThingsSrv/base.tjs"-->
<%@ LANGUAGE=JavaScript %>
<%
	Response.clear();
	Response.write(function(){
		var ret={
			    success:false,
			    data:[],
			    msg:""
		};
		var userID= Request.form("userID");
		var roomID= Request.form("roomID");
		//

	    //return xmlHttp.responseText
	    //ÏÈÅÐ¶ÏÒÑ°ó¶¨
		var res=TSS.JSON.decode(System.RDC.ExcuteSQL("ElectPay",false,"update  \"UserRoom\" set \"T_isBinding\"=1 where \"T_UserID\"='"+userID+"' and \"T_RoomID\"='"+roomID+"'"))[0];
		if(res.MessageCode>0){
		    ret.msg=res.MessageText;
		    return  TSS.JSON.encode(ret);
		}
		
		ret.success=true;
		return TSS.JSON.encode(ret);
	}());
	Response.ContentType = "text/plain";
%>